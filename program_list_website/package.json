{
  "name": "triple-frontend-exercise",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.8.4",
    "json-server": "^0.17.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/parser": "^5.31.0",
    "concurrently": "^5.3.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.7.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "seed-db": "node src/server/initDb",
    "server": "json-server --watch --port 4002 --delay 1500 src/server/db.json",
    "start": "npm run seed-db && concurrently \"npm run server\" \"PORT=3002 react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm run seed-db",
    "lint": "yarn lint:ts && yarn lint:prettier && yarn lint:eslint && yarn lint:stylelint",
    "lint:ts": "tsc",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx ./ --color --cache",
    "lint:prettier": "prettier -c ./",
    "lint:stylelint": "stylelint **/*.{ts,tsx}",
    "healthcheck": "yarn lint && yarn test && yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "prettier/react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
